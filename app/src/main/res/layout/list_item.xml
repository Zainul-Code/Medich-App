<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="8dp"
    android:layout_marginHorizontal="8dp"
    android:background="@color/putih"
    android:foreground="?android:attr/selectableItemBackground">


    <ImageView
        android:id="@+id/apotik1"
        android:layout_width="337dp"
        android:layout_height="123dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.106"
        app:srcCompat="@drawable/apotik1" />

    <ImageView
        android:id="@+id/apotik2"
        android:layout_width="335dp"
        android:layout_height="142dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apotik1"
        app:layout_constraintVertical_bias="0.045"
        app:srcCompat="@drawable/apotik2" />


    <ImageView
        android:id="@+id/apotik3"
        android:layout_width="335dp"
        android:layout_height="142dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apotik2"
        app:layout_constraintVertical_bias="0.045"
        app:srcCompat="@drawable/apotik3" />

    <ImageView
        android:id="@+id/apotik4"
        android:layout_width="335dp"
        android:layout_height="142dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apotik3"
        app:layout_constraintVertical_bias="0.045"
        app:srcCompat="@drawable/apotik4" />

    <ImageView
        android:id="@+id/apotik5"
        android:layout_width="335dp"
        android:layout_height="142dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apotik4"
        app:layout_constraintVertical_bias="0.045"
        app:srcCompat="@drawable/apotik5" />
</androidx.constraintlayout.widget.ConstraintLayout>